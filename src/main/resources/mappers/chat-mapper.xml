<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="Chat">
<!-- 내가 속한 채팅방 시퀀스 목록 가져오기 -->
<select id="getAllChatSeq" parameterType="string" resultType="int">
    select chat_seq from chat_member where member_email = #{member_email}
</select>


</mapper>