<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="Department">

  	<!-- 부서리스트 가져오기 -->
  	<select id="getAllDeptCode" resultType="com.kedu.admin.department.DepartmentDTO">
  		select * from department where company_code = #{company_code}
  	</select>
  		
</mapper>